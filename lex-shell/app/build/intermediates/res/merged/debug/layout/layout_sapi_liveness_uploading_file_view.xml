<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


    <com.baidu.fsg.face.liveness.view.DynamicWaveView
            android:id="@+id/dynamic_wave"
            android:layout_width="136dp"
            android:layout_height="136dp"
            android:layout_centerInParent="true"/>

    <com.baidu.fsg.face.liveness.view.UploadCircleView
            android:id="@+id/my_circle"
            android:layout_width="136dp"
            android:layout_height="136dp"
            android:layout_centerInParent="true"/>


    <ImageView
            android:id="@+id/circle"
            android:layout_width="136dp"
            android:layout_height="136dp"
            android:layout_centerInParent="true"
            android:src="@drawable/sapi_liveness_blue_circle"
            android:visibility="gone"/>

    <ImageView
            android:id="@+id/tick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/sapi_liveness_white_tick"
            android:visibility="gone"/>


</RelativeLayout>